                    </li>
                    <li>
                        <a href="#" ng-click="prev()">«</a>
                    </li>
                </ul>



                <ul ng-repeat="n in current_page_slider" class="pagination">
                    <li>
                        <a href="#" ng-click="update_page(n)"> {{n}}</a>
                    </li>

                </ul>
                <ul class="pagination">



                    <ul ng-if="no_of_pages > 5" class="pagination">

                        <li>
                            <a href="#" ng-click="next()">»</a>
                        </li>
                        <li>
                            <a href="#" ng-click="last_page()">Last</a>
                        </li>
                    </ul>
                </ul>
        </div>
    </div>
